   "source": [
    "with open(\"../data/json.json\", 'r') as fr:\n",
    "    data = json.loads(fr.read())\n",
    "    a = {k: {\"id\":v, \"description\": None} \n",
    "         for k, v in data.items()}\n",
    "    \n",
    "with open(\"../data/json.json\", \"w\") as fw:\n",
    "    json.dump(a, fw, ensure_ascii=False, indent=4)"
   ]
